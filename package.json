{
  "name": "kai-camera",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "develop": "gatsby develop -p 8000",
    "production": "gatsby build && gatsby serve -p 8005 -o",
    "cleanup": "rm -rf .cache public",
    "reload": "yarn cleanup && rm -rf node_modules && yarn",
    "reloadDev": "yarn reload && yarn develop",
    "cleanPort": "fuser -n tcp -k 8000",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint .",
    "lintFix": "eslint . --fix"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^8.0.0",
    "es6-promise": "^4.2.6",
    "gatsby": "^2.7.1",
    "gatsby-image": "^2.1.1",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-netlify": "^2.0.17",
    "gatsby-plugin-netlify-cms": "^4.0.1",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.1.1",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-source-dropbox": "^0.1.1",
    "gatsby-source-filesystem": "^2.0.37",
    "gatsby-transformer-sharp": "^2.1.19",
    "material-design-icons": "^3.0.1",
    "netlify-cms": "^2.9.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-burger-menu": "^2.6.10",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.1",
    "react-images": "^0.5.19",
    "react-lightbox-component": "^1.2.1",
    "react-photo-gallery": "^6.2.1",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "babel-plugin-styled-components": "^1.10.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-compat": "^3.1.2",
    "eslint-plugin-deprecate": "^0.5.4",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0"
  },
  "lint-staged": {
    "*.js": [
      "pretty-quick",
      "eslint --fix",
      "stylelint --fix",
      "git add"
    ],
    "*.{json,html}": [
      "pretty-quick",
      "git add"
    ],
    "*.{css,scss}": [
      "pretty-quick",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
